# compile gcc flags
ASFLAGS = $(MCU) $(AS_DEFS) $(AS_INCLUDES) $(OPT) -Wall -fdata-sections -ffunction-sections

CFLAGS = $(MCU) $(C_DEFS) $(C_INCLUDES) $(OPT) -Wall -fdata-sections -ffunction-sections

ifeq ($(DEBUG), 1)
CFLAGS += -g -gdwarf-2
OPT = -Og -g
else
OPT = -O2  # or -O3, or -Os if size is critical
endif


# Generate dependency information
CFLAGS += -MMD -MP -MF"$(@:%.o=%.d)"


#######################################
# LDFLAGS
#######################################
# link script
LDSCRIPT = stm32f407zetx_flash.ld

# libraries
LIBS = -lc -lm -lnosys 
LIBDIR = 
LDFLAGS = $(MCU) -specs=nano.specs -T$(LDSCRIPT) $(LIBDIR) $(LIBS) -Wl,-Map=$(BUILD_DIR)/$(TARGET).map,--cref -Wl,--gc-sections

# default action: build all
all: $(BUILD_DIR)/$(TARGET).elf $(BUILD_DIR)/$(TARGET).hex $(BUILD_DIR)/$(TARGET).bin


#######################################
# build the application
#######################################
# list of objects
# OBJECTS = $(addprefix $(BUILD_DIR)/,$(notdir $(C_SOURCES:.c=.o)))
# OBJECTS = $(addprefix $(BUILD_DIR)/, $(patsubst %.c, %$(BUILD_DIR)/%.o, $(C_SOURCES)))
# OBJECTS = $(patsubst $(dir $(C_SOURCES))%, $(BUILD_DIR)/%, $(C_SOURCES:.c=.o))
OBJECTS = $(patsubst %.c, $(BUILD_DIR)/%.o, $(C_SOURCES))

vpath %.c $(sort $(dir $(C_SOURCES)))
# $(info OBJECTS = $(OBJECTS))

# list of ASM program objects
OBJECTS += $(addprefix $(BUILD_DIR)/,$(notdir $(ASM_SOURCES:.s=.o)))
vpath %.s $(sort $(dir $(ASM_SOURCES)))

# Automatically create the build directory for each object
prep: $(sort $(dir $(OBJECTS)))

# Rule to create directories
$(sort $(dir $(OBJECTS))):
	@mkdir -p $@

$(BUILD_DIR)/%.o: %.c Makefile
	@echo "\033[1mGCC\033[0m $<"
	@$(CC) -c $(CFLAGS) -Wa,-a,-ad,-alms=$(@:.o=.lst) $< -o $@

$(BUILD_DIR)/%.o: %.s Makefile
	@echo "\033[1mASM\033[0m $<"
	@$(AS) -c $(CFLAGS) $< -o $@

$(BUILD_DIR)/$(TARGET).elf: prep $(OBJECTS) Makefile 
	@echo "\033[1mLD\033[0m $@"
	@$(CC) $(OBJECTS) $(LDFLAGS) -o $@
	@$(SZ) $@

$(BUILD_DIR)/%.hex: $(BUILD_DIR)/%.elf
	@echo "\033[1mHEX\033[0m $@"
	@$(HEX) $< $@
	
$(BUILD_DIR)/%.bin: $(BUILD_DIR)/%.elf
	@echo "\033[1mBIN\033[0m $@"
	@$(BIN) $< $@	
	
#######################################
# clean up
#######################################
clean:
	-rm -fR $(BUILD_DIR)
  
#######################################
# dependencies
#######################################
-include $(wildcard $(BUILD_DIR)/*.d)

JLINK = JLinkExe
FLASH_ADDR = 0x08000000

flash: $(BUILD_DIR)/$(TARGET).bin
	@echo "\033[1mFlashing...\033[0m $<"
	@echo "r\nloadfile $< ${FLASH_ADDR}\nr\nq" | $(JLINK) -device STM32F407VG -if SWD -speed 4000 > /dev/null 2>&1

